{
	"htmlWhitespaceSensitivity": "ignore",
	"bracketSameLine": false,
	"printWidth": 100,
	"singleQuote": false,
	"jsxSingleQuote": true,
	"trailingComma": "all",
	"useTabs": true,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
